@model IEnumerable<GummyBearKingdom.Models.Product>

@{
    ViewData["Title"] = "Gummy Bear Kingdom";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Our products:</h2>
<ul>
    @foreach (var listItem in Model)
    {

        <li>
            @Html.ActionLink(listItem.ProductName, "Details", new { id = listItem.ProductId })
        </li>
    }
</ul>